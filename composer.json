{
    "name": "geocoder-php/provider-integration-tests",
    "description": "Integration tests Geocoder provider",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "geocoder",
        "provider",
        "test"
    ],
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com",
            "homepage": "https://github.com/nyholm"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "nyholm/psr7": "^1.0",
        "php-http/curl-client": "^2.2",
        "php-http/mock-client": "^1.2",
        "phpunit/phpunit": "^9.6.17 || ^10.5 || ^11.0.4",
        "psr/http-client": "^1.0"
    },
    "require-dev": {
        "geocoder-php/nominatim-provider": "^5.3",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Geocoder\\IntegrationTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geocoder\\IntegrationTest\\Test\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    }
}
